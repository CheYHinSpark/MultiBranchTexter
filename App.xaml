<Application x:Class="MultiBranchTexter.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MultiBranchTexter">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="BaseDictionary" Source="Resources/Dictionary.xaml"/>
                <ResourceDictionary Source="Resources/WindowResources.xaml"/>
                <ResourceDictionary Source="Resources/FCCResources.xaml"/>
                <!--把换色字典放在最后-->
                <ResourceDictionary Source="Resources/LightColorDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

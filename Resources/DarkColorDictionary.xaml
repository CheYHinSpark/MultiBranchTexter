<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--深浅色-->
    <SolidColorBrush x:Key="Text.Foreground.BaseBrush" Color="White"/>
    <SolidColorBrush x:Key="Foreground.ThemeBrush" Color="Black"/>
    <SolidColorBrush x:Key="Foreground.BaseBrush" Color="{DynamicResource Theme}"/>
    <SolidColorBrush x:Key="Highlight.BaseBrush" Color="#666666"/>
    <!--工作界面背景颜色-->
    <SolidColorBrush x:Key="MainBrush" Color="#111111"/>
    <SolidColorBrush x:Key="BaseBrush" Color="#222222"/>
    <SolidColorBrush x:Key="NodeBgBrush" Color="#FF241848"/>
    <SolidColorBrush x:Key="NodeBgHBrush" Color="#FF423B64"/>
</ResourceDictionary>